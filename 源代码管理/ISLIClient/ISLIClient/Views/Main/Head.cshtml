
@{
    Layout = null;
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>人格障碍诊断</title>
    <link rel="stylesheet" type="text/css" href="~/css/public.css" />
    <script type="text/javascript" src="~/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/js/public.js"></script>
    <script type="text/javascript" src="~/js/external.js"></script>

</head>

<body>
    <!-- 头部 -->
    <div class="head">
        <div class="headL">
            <img class="headLogo" src="~/images/banner2.png" />
        </div>
        <div class="headR">
            <a href="/main/Logout" target="_parent">【退出】</a>
        </div>
    </div>
    <div class="user">
        @if (ViewBag.LoginInfo.UserTypeId == 1)
        {
            <span>@ViewBag.LoginInfo.PublisheChinese</span>
        }
        else if (ViewBag.LoginInfo.UserTypeId == 2)
        {
            <span>@ViewBag.LoginInfo.CompanyName</span>
        }
        else
        {
            <span>@ViewBag.LoginInfo.UserName</span>
        }
    </div>

</body>

</html>
